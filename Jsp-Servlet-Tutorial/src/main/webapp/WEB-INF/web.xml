<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>Jsp-Servlet-Tutorial</display-name>
	<servlet>
		<servlet-name>addSvl</servlet-name>
		<servlet-class>com.add.AddServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>addSvl</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>pqr</servlet-name>
		<servlet-class>com.add.SqServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>pqr</servlet-name>
		<url-pattern>/sq</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>mysv</servlet-name>
		<servlet-class>com.svcontexts.MyServlet</servlet-class>
		<init-param>
			<param-name>name</param-name>
			<param-value>Roger</param-value>
		</init-param>
	</servlet>

	<servlet-mapping>
		<servlet-name>mysv</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>

	<context-param>
		<param-name>name</param-name>
		<param-value>LocDang</param-value>
	</context-param>

	<context-param>
		<param-name>phone</param-name>
		<param-value>Iphone</param-value>
	</context-param>

</web-app>